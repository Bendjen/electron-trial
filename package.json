{
  "name": "electron-trial",
  "version": "1.0.0",
  "description": "",
  "main": "app/main/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./scripts/dev.js",
    "dist": "cross-env NODE_ENV=production node ./scripts/prod.js && electron-builder --dir",
    "pack": "cross-env NODE_ENV=production node ./scripts/prod.js && electron-builder"
  },
  "build": {
    "productName": "electron-trial",
    "appId": "Bendjen",
    "directories": {
      "output": "dist"
    },
    "files": [
      "app",
      "extensions"
    ],
    "mac": {
      "icon": "icons/512x512.png"
    },
    "win": {
      "icon": "icons/512x512.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/512x512.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./icons/installerIcon.ico",
      "uninstallerIcon": "./icons/uninstallerIcon.ico",
      "installerHeaderIcon": "./icons/installerIcon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "releaseInfo": {
			"releaseName": "",
			"releaseNotes": "版本更新测试"
		},
    "publish": [
      {
        "provider": "github",
        "url": "https://raw.githubusercontent.com/Bendjen/electron-trial/master/release/"
      }
    ]
  },
  "author": "Bendjen",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.11.2",
    "babili-webpack-plugin": "^0.1.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^15.5.5",
    "electron-builder": "^20.40.2",
    "electron-connect": "^0.6.3",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^2.0.0",
    "react-hot-loader": "^4.8.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-dev-server": "3.5.1"
  },
  "dependencies": {
    "antd": "^3.19.1",
    "axios": "^0.19.0",
    "electron-updater": "^4.0.6",
    "flex.css": "^1.1.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  }
}
